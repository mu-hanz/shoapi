{
    "name": "muhanz/shoapi",
    "description": "Laravel Shopee Open API SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "Mu Hanz",
            "email": "muhanz21@gmail.com",
            "homepage": "https://github.com/mu-hanz"
        }
    ],
    "homepage": "https://github.com/mu-hanz/shoapi",
    "keywords": ["Laravel", "Shoapi", "Shopee API"],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Muhanz\\Shoapi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muhanz\\Shoapi\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "laravel": {
            "providers": [
                "Muhanz\\Shoapi\\ShoapiServiceProvider"
            ],
            "aliases": {
                "Shoapi": "Muhanz\\Shoapi\\Facades\\Shoapi"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
